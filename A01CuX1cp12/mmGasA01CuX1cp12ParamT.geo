/// CONSTANTS
//----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//// Variable designation: geometrical element, structural reference, interface / description, number

//----------------------------------------------------------
//// Geometric parameters

cA00MWf = 1.00;																																					// mesh window factor
cA00MM = 1.00;																																					// geometrical scaling

dA00Extx = 5.25;																																				// shell x dimension x d, in mm
dA00Exty = 5.25;																																				// shell y dimension y d, in mm

//----------------------------------------------------------
//// Current best dimensions for mesh characteristic lengths

lcA01cp2Cnd1 = 0.02;																																			// characterization of metal surfaces / copper conductor boundary

//----------------------------------------------------------
//// Copper anode geometric parameters

cA01CuX2cp12GeoFx = 1;																																			// geometric factor
cA01CuX2cp12GeoFy = 1;																																			// geometric factor

/// Copper strip width

dA01cpStrpWth = 0.95*cA00MM;																																	// copper anode strip width
dA01cpSpWth1 = 0.05*cA00MM;																																		// copper strip x separation width
dA01cpSpWth2 = 0.05*cA00MM;																																		// copper strip y separation width

/// A01 counter constant

mA01c1 = 0;																																						// counter / constant, copper x1
nA01c1 = 0;																																						// counter / constant, copper y1
mA01c2 = 1;																																						// counter / constant, copper x2
nA01c2 = 1;																																						// counter / constant, copper y2

//----------------------------------------------------------
//// Geometric copper anode parameters

cA01DetShellGeoFx = 1;																																			// geometric factor
cA01DetShellGeoFy = 1;																																			// geometric factor

/// A01 detector shell counter constant

mA01cp12DetShellc1 = 0;																																			// counter / constant, copper x1
nA01cp12DetShellc1 = 0;																																			// counter / constant, copper y1
mA01cp12DetShellc2 = 1;																																			// counter / constant, copper x2
nA01cp12DetShellc2 = 1;																																			// counter / constant, copper y2

//----------------------------------------------------------
//// Copper anode parameters

/// Detector size co-ordinate

dA01DetSzxCo = ( dA00Extx - 0*( dA01cpStrpWth + dA01cpSpWth1 ) );																								// detector size x co-ordinate
dA01DetSzyCo = ( dA00Exty - 0*( dA01cpStrpWth + dA01cpSpWth2 ) );																								// detector size y co-ordinate

/// Copper anode size co-ordinate

dA01cpAnSzxCo1a = ( 1*dA01cpSpWth1/2 );																															// copper anode size x 1a co-ordinate
dA01cpAnSzyCo1a = ( 1*dA01cpSpWth2/2 );																															// copper anode size y 1a co-ordinate

dA01cpAnSzxCo1b = ( (dA01cpSpWth1/2) + ( dA01cpStrpWth ) );																										// copper anode size x 1b co-ordinate
dA01cpAnSzyCo1b = ( dA00Exty - dA01cpSpWth2/2 );																												// copper anode size y 1b co-ordinate

dA01cpAnSzxCo2a = ( dA00Extx - dA01cpSpWth1/2 );																												// copper anode size x 2a co-ordinate
dA01cpAnSzyCo2a = ( dA00Exty - dA01cpSpWth2/2 );																												// copper anode size y 2a co-ordinate

/// Copper strip length

dA01cpStrpLth = ( dA00Exty - dA01cpSpWth2 );																													// copper strip length

// dA01cpAnSzyCo1b = ( ( dA01cpSpWth2/2 ) + ( dA01cpStrpLth ) );																								// copper anode size y 1b co-ordinate

/// Copper strip number x

nA01cpNumStrpx = Floor ( dA01DetSzxCo / ( dA01cpStrpWth + dA01cpSpWth1 ) );																						// copper strip number x

/// Copper anode co-ordinate

dA01cpxCoD1 = ( dA01DetSzxCo - dA01cpSpWth1 - dA01cpStrpWth - ( ( nA01cpNumStrpx - 1 )*( dA01cpStrpWth + dA01cpSpWth1 ) ) )/2;									// copper anode x co-ordinate distance 1
dA01cpyCoD2 = dA01cpSpWth2/2;																																	// copper anode y co-ordinate distance 2

/// Geometric constant

dA01GeoWcxr = dA01cpxCoD1*1.0;																																	// geometric constant, y - direction strip in the x radial direction
dA01GeoWcyd = dA01cpyCoD2*0.0;																																	// geometric constant, y - direction strip in the y - direction

/// Copper strip count x

nA01cpStrpCntx = Floor ( ( dA01DetSzxCo ) / ( dA01cpStrpWth + dA01cpSpWth1 ) );																					// strip count x

/// Copper anode units initial

numA01cpUx0 = nA01cpStrpCntx;																																	// number of copper anode x units initial
numA01cpUy0 = 1;																																				// number of copper anode y units initial 

/// Copper anode units 
	
numA01cpUx = numA01cpUx0 - 1;																																	// number of copper anode units in the y - direction
numA01cpUy = numA01cpUy0 - 1;																																	// number of copper anode units in the x - direction

//----------------------------------------------------------
//// Level parameters

lvlA00cp1_X2cp12 = 0;																																			// lvl 0cp1 electrode ground bottom copper level, in mm

thA00cp12_X2cp12 = 0.10;																																		// 0cp12 thickness, in mm

lvlA00cp1_X2cp12 = ( lvlA00cp1_X2cp12 );																														// lvl 0cp1 electrode ground bottom copper level, in mm
lvlA00cp2_X2cp12 = ( lvlA00cp1_X2cp12 + thA00cp12_X2cp12 );																										// lvl 0pcb1 / 0cp2 electrode ground top copper level / thickness, in mm

thA00pcb12_X2cp12 = 0.25;																																		// 0pcb12 thickness, in mm
thA01cp12_X2cp12 = 0.10;																																		// 1cp12 thickness, in mm

lvlA01cp1_X2cp12 = ( lvlA00cp2_X2cp12 + thA00pcb12_X2cp12 );																									// lvl 0pcb2 / 1pcb1 / 1cp1 bottom electrode copper level / thickness, in mm
lvlA01cp2_X2cp12 = ( lvlA01cp1_X2cp12 + thA01cp12_X2cp12 );																										// lvl 1pcb2 / 2pcb1 / 1cp2 top electrode copper level / thickness, in mm

thA01pcb12_X2cp12 = 0.25;																																		// 2pcb12 thickness, in mm
thA01cp12 = 0.10;																																				// 2cp12 thickness, in mm

lvlA01cp1 = ( lvlA01cp2_X2cp12 + thA01pcb12_X2cp12 );																											// lvl 2pcb2 / 2cp1 bottom electrode copper level / thickness, in mm
lvlA01cp2 = ( lvlA01cp1 + thA01cp12 );																															// lvl 2cp2 top electrode copper level / thickness, in mm


