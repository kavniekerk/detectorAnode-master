// GEOMETRY MODULE
/// COPPER STRIP STRUCTURE
//----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//// Variable designation: geometrical element, structural reference, interface / description, number


/// CONSTANTS
//----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//// Variable designation: geometrical element, structural reference, interface / description, number

//----------------------------------------------------------
//// Geometric parameters

cA00MWf = 1.00;																																					// mesh window factor
cA00MM = 1.00;																																					// geometrical scaling

dA00Extx = 5.25;																																				// shell x dimension x d, in mm
dA00Exty = 5.25;																																				// shell y dimension y d, in mm

//----------------------------------------------------------
//// Current best dimensions for mesh characteristic lengths

lcA00cp12Cnd1 = 0.02;																																			// characterization of metal surfaces / copper conductor boundary

//----------------------------------------------------------
//// Copper anode geometric parameters

cA00CuY1cp12GeoFx = 1;																																			// geometric factor
cA00CuY1cp12GeoFy = 1;																																			// geometric factor

/// Copper strip width

dA00cpStrpWth = 0.95*cA00MM;																																	// copper anode strip width
dA00cpSpWth1 = 0.05*cA00MM;																																		// copper strip y separation width
dA00cpSpWth2 = 0.05*cA00MM;																																		// copper strip x separation width

/// A00 counter constant

mA00c1 = 0;																																						// counter / constant, copper y1
nA00c1 = 0;																																						// counter / constant, copper x1
mA00c2 = 1;																																						// counter / constant, copper y2
nA00c2 = 1;																																						// counter / constant, copper x2

//----------------------------------------------------------
//// Geometric copper anode parameters

cA00cp12DetShellGeoFx = 1;																																		// geometric factor
cA00cp12DetShellGeoFy = 1;																																		// geometric factor

/// A00 detector shell counter constant

mA00cp12DetShellc1 = 0;																																			// counter / constant, copper x1
nA00cp12DetShellc1 = 0;																																			// counter / constant, copper y1
mA00cp12DetShellc2 = 1;																																			// counter / constant, copper x2
nA00cp12DetShellc2 = 1;																																			// counter / constant, copper y2

//----------------------------------------------------------
//// Copper anode parameters

/// Detector size co-ordinate

dA00DetSzyCo = ( dA00Exty - 0*( dA00cpStrpWth + dA00cpSpWth1 ) );																								// detector size y co-ordinate
dA00DetSzxCo = ( dA00Extx - 0*( dA00cpStrpWth + dA00cpSpWth2 ) );																								// detector size x co-ordinate

/// Copper anode size co-ordinate

dA00cpAnSzyCo1a = ( 1*dA00cpSpWth1/2 );																															// copper anode size y 1a co-ordinate
dA00cpAnSzxCo1a = ( 1*dA00cpSpWth2/2 );																															// copper anode size x 1a co-ordinate

dA00cpAnSzyCo1b = ( (dA00cpSpWth1/2) + ( dA00cpStrpWth ) );																										// copper anode size y 1b co-ordinate
dA00cpAnSzxCo1b = ( dA00Extx - dA00cpSpWth2/2 );																												// copper anode size x 1b co-ordinate

dA00cpAnSzyCo2a = ( dA00Exty - dA00cpSpWth1/2 );																												// copper anode size y 2a co-ordinate
dA00cpAnSzxCo2a = ( dA00Extx - dA00cpSpWth2/2 );																												// copper anode size x 2a co-ordinate

/// Copper strip length

dA00cpStrpLth = ( dA00Extx - dA00cpSpWth2 );																													// copper strip length

// dA00cpAnSzxCo1b = ( ( dA00cpSpWth2/2 ) + ( dA00cpStrpLth ) );																								// copper anode size x 1b co-ordinate

/// Copper strip number y

nA00cpNumStrpy = Floor ( dA00DetSzyCo / ( dA00cpStrpWth + dA00cpSpWth1 ) );																						// copper strip number y

/// Copper anode co-ordinate

dA00cpyCoD1 = ( dA00DetSzyCo - dA00cpSpWth1 - dA00cpStrpWth - ( ( nA00cpNumStrpy - 1 )*( dA00cpStrpWth + dA00cpSpWth1 ) ) )/2;									// copper anode y co-ordinate distance 1
dA00cpxCoD2 = dA00cpSpWth2/2;																																	// copper anode x co-ordinate distance 2

/// Geometric constant

dA00GeoWcyr = dA00cpyCoD1*1.0;																														 			// geometric constant, x - direction strip in the y radial direction
dA00GeoWcxd = dA00cpxCoD2*0.0;																														 			// geometric constant, x - direction strip in the x - direction

/// Copper strip count y

nA00cpStrpCnty = Floor ( ( dA00DetSzyCo ) / ( dA00cpStrpWth + dA00cpSpWth1 ) );																					// strip count y

/// Copper anode units initial

numA00cpUy0 = nA00cpStrpCnty;																																	// number of copper anode y units initial
numA00cpUx0 = 1;																																				// number of copper anode x units initial 

/// Copper anode units

numA00cpUy = numA00cpUy0 - 1;																																	// number of copper anode units in the x - direction
numA00cpUx = numA00cpUx0 - 1;																																	// number of copper anode units in the y - direction

//----------------------------------------------------------
//// Level parameters

lvlA00cp1_Y1cp12 = 0;																																			// lvl 0cp1 electrode ground bottom copper level, in mm

thA00cp12_Y1cp12 = 0.00;																																		// 0cp12 thickness, in mm

lvlA00cp1_Y1cp12 = ( lvlA00cp1_Y1cp12 );																														// lvl 0cp1 electrode ground bottom copper level, in mm
lvlA00cp2_Y1cp12 = ( lvlA00cp1_Y1cp12 + thA00cp12_Y1cp12 );																										// lvl 0pcb1 / 0cp2 electrode ground top copper level / thickness, in mm

thA00pcb12_Y1cp12 = 0.00;																																		// 0pcb12 thickness, in mm
thA00cp12 = 0.10;																																				// 1cp12 thickness, in mm

lvlA00cp1 = ( lvlA00cp2_Y1cp12 + thA00pcb12_Y1cp12 );																											// lvl 0pcb2 / 1pcb1 / 1cp1 bottom electrode copper level / thickness, in mm
lvlA00cp2 = ( lvlA00cp1 + thA00cp12 );																															// lvl 1pcb2 / 2pcb1 / 1cp2 top electrode copper level / thickness, in mm


/// VARIABLE DECLARATION
//----------------------------------------------------------
//// Lines & surfaces

lA00cp1Bb1A[] = {};
lA00cp1Bb2A[] = {};
lA00cp1Bb3A[] = {};
lA00cp1Bb4A[] = {};

lA00cp2Tb1A[] = {};
lA00cp2Tb2A[] = {};
lA00cp2Tb3A[] = {};
lA00cp2Tb4A[] = {};

lA00cp12Ib1A[] = {};
lA00cp12Ib2A[] = {};
lA00cp12Ib3A[] = {};
lA00cp12Ib4A[] = {};

surfA00cp12Ib1A[] = {};
surfA00cp12Ib2A[] = {};
surfA00cp12Ib3A[] = {};
surfA00cp12Ib4A[] = {};

surfA00cp1Bb5A[] = {};
surfA00cp2Tb6A[] = {};

slA00cpSb1[] = {};


/// GEOMETRY MODULE
/// COPPER STRIP STRUCTURE
//----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//// Variable designation: geometrical element, structural reference, interface / description, number


/// POINTS
//----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//// External shell corners


/// LEVEL 1cp1 - copper
//----------------------------------------------------------
//// Copper strip corner 1

ptExA00cp1C1 = newp; Point ( ptExA00cp1C1 ) = { cA00cp12DetShellGeoFx*0 + cA00cp12DetShellGeoFx*mA00cp12DetShellc1*dA00Extx, cA00cp12DetShellGeoFy*0 + cA00cp12DetShellGeoFy*nA00cp12DetShellc1*dA00Exty, lvlA00cp1, lcA00cp12Cnd1 };				

/// Lvl 1cp1 - level 1 bottom layer - middle                                                                                                           
/// Copper electrode strips

//----------------------------------------------------------
//// Copper strip corner 2

ptExA00cp1C2 = newp; Point ( ptExA00cp1C2 ) = { cA00cp12DetShellGeoFx*0 + cA00cp12DetShellGeoFx*mA00cp12DetShellc1*dA00Extx, cA00cp12DetShellGeoFy*dA00Exty + cA00cp12DetShellGeoFy*nA00cp12DetShellc1*dA00Exty, lvlA00cp1, lcA00cp12Cnd1 };               

/// Lvl 1cp1 - level 1 bottom layer - middle 
/// Copper electrode strips

//----------------------------------------------------------
//// Copper strip corner 3

ptExA00cp1C3 = newp; Point ( ptExA00cp1C3 ) = { cA00cp12DetShellGeoFx*dA00Extx + cA00cp12DetShellGeoFx*mA00cp12DetShellc1*dA00Extx, cA00cp12DetShellGeoFy*dA00Exty + cA00cp12DetShellGeoFy*nA00cp12DetShellc1*dA00Exty, lvlA00cp1, lcA00cp12Cnd1 };             

/// Lvl 1cp1 - level 1 bottom layer - middle 
/// Copper electrode strips

//----------------------------------------------------------
//// Copper strip corner 4

ptExA00cp1C4 = newp; Point ( ptExA00cp1C4 ) = { cA00cp12DetShellGeoFx*dA00Extx + cA00cp12DetShellGeoFx*mA00cp12DetShellc1*dA00Extx, cA00cp12DetShellGeoFy*0 + cA00cp12DetShellGeoFy*nA00cp12DetShellc1*dA00Exty, lvlA00cp1, lcA00cp12Cnd1 };               

/// Lvl 1cp1 - level 1 bottom layer - middle 
/// Copper electrode strips


/// LEVEL 1cp2 - copper
//----------------------------------------------------------
//// Copper strip corner 1

ptExA00cp2C1 = newp; Point ( ptExA00cp2C1 ) = { cA00cp12DetShellGeoFx*0 + cA00cp12DetShellGeoFx*mA00cp12DetShellc1*dA00Extx, cA00cp12DetShellGeoFy*0 + cA00cp12DetShellGeoFy*nA00cp12DetShellc1*dA00Exty, lvlA00cp2, lcA00cp12Cnd1 };

/// Lvl 1cp2 - level 1 top layer

//----------------------------------------------------------
//// Copper strip corner 2

ptExA00cp2C2 = newp; Point ( ptExA00cp2C2 ) = { cA00cp12DetShellGeoFx*0 + cA00cp12DetShellGeoFx*mA00cp12DetShellc1*dA00Extx, cA00cp12DetShellGeoFy*dA00Exty + cA00cp12DetShellGeoFy*nA00cp12DetShellc1*dA00Exty, lvlA00cp2, lcA00cp12Cnd1 };

/// Lvl 1cp2 - level 1 top layer

//----------------------------------------------------------
//// Copper strip corner 3

ptExA00cp2C3 = newp; Point ( ptExA00cp2C3 ) = { cA00cp12DetShellGeoFx*dA00Extx + cA00cp12DetShellGeoFx*mA00cp12DetShellc1*dA00Extx, cA00cp12DetShellGeoFy*dA00Exty + cA00cp12DetShellGeoFy*nA00cp12DetShellc1*dA00Exty, lvlA00cp2, lcA00cp12Cnd1 };

/// Lvl 1cp2 - level 1 top layer

//----------------------------------------------------------
//// Copper strip corner 4

ptExA00cp2C4 = newp; Point ( ptExA00cp2C4 ) = { cA00cp12DetShellGeoFx*dA00Extx + cA00cp12DetShellGeoFx*mA00cp12DetShellc1*dA00Extx, cA00cp12DetShellGeoFy*0 + cA00cp12DetShellGeoFy*nA00cp12DetShellc1*dA00Exty, lvlA00cp2, lcA00cp12Cnd1 };

/// Lvl 1cp2 - level 1 top layer


/// POINTS
//----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//// Individual strip corners


/// LEVEL 1cp1 - copper

For m1 In { 0:numA00cpUy }
  For n1 In { 0:numA00cpUx }


/// Level 3 - top lower layer
//----------------------------------------------------------
//// Copper strip corner 1

ptA00cp1C1~{m1}~{n1} = newp; Point ( ptA00cp1C1~{m1}~{n1} ) = { dA00GeoWcxd + cA00CuY1cp12GeoFx*dA00cpAnSzxCo1a + cA00CuY1cp12GeoFx*n1*( dA00cpStrpLth ), dA00GeoWcyr + cA00CuY1cp12GeoFy*dA00cpAnSzyCo1a + cA00CuY1cp12GeoFy*m1*( dA00cpStrpWth + dA00cpSpWth1 ), lvlA00cp1, lcA00cp12Cnd1 };

//----------------------------------------------------------
