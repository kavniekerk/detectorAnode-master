ptA00cp2C1~{m1}~{n1} = newp; Point ( ptA00cp2C1~{m1}~{n1} ) = { dA00GeoWcxd + cA00CuY1cp12GeoFx*dA00cpAnSzxCo1a + cA00CuY1cp12GeoFx*n1*( dA00cpStrpLth ), dA00GeoWcyr + cA00CuY1cp12GeoFy*dA00cpAnSzyCo1a + cA00CuY1cp12GeoFy*m1*( dA00cpStrpWth + dA00cpSpWth1 ), lvlA00cp2, lcA00cp12Cnd1 };

//----------------------------------------------------------
